{
  "name": "node_start_template",
  "version": "0.1.0",
  "description": "project skeleton to pull from.",
  "main": "index.js",
  "babel": {
    "presets": [
      "jsy"
    ]
  },
  "scripts": {
    "start": "node dist/main.js",

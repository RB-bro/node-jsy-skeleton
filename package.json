  },
  "author": "brandon brown",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-jsy": "^0.7.2",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "tap-lite-tester": "^0.3.9"
  }
}
